{
    "name": "sfneal/redis-helpers",
    "description": "Boilerplate for PHP package repo's",
    "keywords": [
        "sfneal",
        "redis-helpers"
    ],
    "homepage": "https://github.com/sfneal/redis-helpers",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Stephen Neal",
            "email": "stephen.neal14@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/redis": ">=5.0",
        "illuminate/support": ">=5.0",
        "sfneal/actions": ">=0.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.0.0"
    },
    "autoload": {
        "psr-4": {
            "Sfneal\\Helpers\\Redis\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Sfneal\\Helpers\\Redis\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
